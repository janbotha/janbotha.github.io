   $.fn.productCategorySlider = function () {

       return this.each(function () {
           var $productCategorySlider = $(this);

           var $productCategorySliderTiles = $productCategorySlider.find('.carousel-inner-product');
           var $numberOfImages = $productCategorySliderTiles.find('.carousel-card-product').length;

           $(window).on('resize', function() {
               if ($productCategorySliderTiles.length) {
                   var wrapperWidth = $productCategorySlider.width() - 34;
                   var windowWidth = window.innerWidth;
                   $productCategorySlider.css({ 'margin-right':'0px' });
                   if (windowWidth <= 400) {
                       $productCategorySliderTiles.find('.carousel-card-img-product').width((wrapperWidth / 2.25), true);
                   } else if ((windowWidth > 400) && (windowWidth < 768)) {
                       $productCategorySliderTiles.find('.carousel-card-img-product').width((wrapperWidth / 2.8), true);
                   } else if ((windowWidth >= 768) && (windowWidth < 1024)) {
                      if ($numberOfImages < 4) {
                          $productCategorySlider.css({ 'margin-right':'32px' });
                          wrapperWidth = wrapperWidth + 4;
                          $productCategorySliderTiles.find('.carousel-card-img-product').width((wrapperWidth / 3), true);
                      } else if ($numberOfImages == 4) {
                          $productCategorySlider.css({ 'margin-right':'32px' });
                          wrapperWidth = wrapperWidth - 11;
                          $productCategorySliderTiles.find('.carousel-card-img-product').width((wrapperWidth / 4), true);
                      } else {
                          wrapperWidth = wrapperWidth - 26;
                          $productCategorySliderTiles.find('.carousel-card-img-product').width((wrapperWidth / 4.5), true);
                      }
                   }
               }
           });

