	//Extensions for the carousel application at NEXT
	$('.cm-carousel').each(function () {
		var carousel = this;

		//Resize kaltura video in hero carousel. - Window resize
		$(window).resize(function () {
			var player = $(carousel).find('.kaltura-video');

			//if the window is resized whilst currently displaying the kaltura video...
			if (player.closest('.item').hasClass('active')) {
				var playerWidth = $(carousel).attr("data-aspect-ratio-width");
				var playerHeight = $(carousel).attr("data-aspect-ratio-height");
				player.css('height', player.width() / playerWidth * playerHeight + 'px');
			} else { //if resized whilst not showing the kaltura video...
				//Adjust the player's height to be the same as the carousel's new height.
				var itemHeight = player.closest('.carousel-inner').height();
				player.css('height', itemHeight + 'px');
			}
		});

		//Resize kaltura video in hero carousel - Window onload
		$(window).on('load', function () {
			var player = $(carousel).find('.kaltura-video');

			//if the window is resized whilst currently displaying the kaltura video...
			if (player.closest('.item').hasClass('active')) {
				var playerWidth = $(carousel).attr("data-aspect-ratio-width");
				var playerHeight = $(carousel).attr("data-aspect-ratio-height");
				player.css('height', player.width() / playerWidth * playerHeight + 'px');
			} else { //if resized whilst not showing the kaltura video...
				//Adjust the player's height to be the same as the carousel's new height.
				var itemHeight = player.closest('.carousel-inner').height();
				player.css('height', itemHeight + 'px');
			}
		});

		//Pause carousel if controls clicked.
		$(carousel).find('.carousel-control').click(function (e) {
			try {
				$(carousel).carousel('pause');
			} catch (e) {}
		});

		$(carousel).find('.carousel-indicators li').click(function (e) {
			try {
				$(carousel).carousel('pause');
			} catch (e) {}
		});
	});

